{
  "name": "filigran-xtm-browser-extension",
  "version": "0.0.12",
  "description": "Filigran XTM Browser Extension - Connect to OpenCTI and OpenAEV for threat intelligence management",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:chrome": "cross-env BROWSER=chrome vite build --watch",
    "build": "cross-env BROWSER=chrome vite build",
    "build:chrome": "cross-env BROWSER=chrome vite build",
    "build:firefox": "cross-env BROWSER=firefox vite build",
    "build:edge": "cross-env BROWSER=edge vite build",
    "build:all": "npm run build:chrome && npm run build:firefox && npm run build:edge",
    "preview": "vite preview",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "icons": "node scripts/generate-icons.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:opencti": "vitest run --config vitest.config.opencti.ts",
    "test:openaev": "vitest run --config vitest.config.openaev.ts",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/geologica": "^5.2.8",
    "@fontsource/ibm-plex-sans": "^5.2.8",
    "@mozilla/readability": "^0.6.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "jspdf": "^3.0.4",
    "mdi-material-ui": "^7.9.4",
    "pdfjs-dist": "^5.4.449",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/chrome": "^0.1.32",
    "@types/mozilla-readability": "^0.2.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "sharp": "^0.34.5",
    "terser": "^5.44.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "author": "Filigran",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/FiligranHQ/xtm-browser-extension"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
